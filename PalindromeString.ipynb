   "source": [
    "### Check whether a string palindrome or not"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "id": "d40cf3d9-42c9-4f73-bcd9-ca4cab0f11bc",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      " 54668898\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Non Palindrome\n"
     ]
    }
   ],
   "source": [
    "s = input()\n",
    "err=0\n",
    "for i in range(int(len(s)/2)):\n",
    "    if s[i]!=s[len(s)-i-1]:\n",
    "        err+=1\n",
    "\n",
    "if err==0:\n",
    "    print(\"Palindrome\")\n",
    "else:\n",
    "    print(\"Non Palindrome\")"
   ]
  }
