   "source": [
    "### Factorial of a Number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "b664fcec-5064-42e9-acd4-7598a6adf5f5",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a number to calculate factorial:  8\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "40320\n"
     ]
    }
   ],
   "source": [
    "def factorial(n):\n",
    "    if n==0 or n==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial(n-1)\n",
    "\n",
    "n = int(input(\"Enter a number to calculate factorial: \"))\n",
    "print(factorial(n))"
   ]
